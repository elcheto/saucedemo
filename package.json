{
  "name": "home-task",
  "version": "1.0.0",
  "description": "cypress with cucumber for https://www.saucedemo.com/",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec \"**/*.feature\"",
    "test:chrome": "cypress run --spec \"**/*.feature\" --browser chrome",
    "test:firefox": "cypress run --spec \"**/*.feature\" --browser firefox",
    "test:open": "cypress open --env screenshots=true,mode=\"headed\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Lu",
  "license": "MIT",
  "dependencies": {
    "cypress-cucumber-preprocessor": "^4.3.1",
    "git": "^0.1.5",
    "typescript": "^5.2.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "^13.6.4"
  }
}
